{
  "name": "triplate-app",
  "version": "0.1.0",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "dev": "concurrently -n=nextjs,storybook,firebase -c=green,'#ff528c',red \"yarn dev:next\" \"yarn dev:storybook\" \"yarn dev:firebase\"",
    "dev:next": "next dev",
    "dev:storybook": "storybook dev --quiet -p 6006",
    "dev:firebase": "cd /home/app/firebase && firebase emulators:start --import=./db/local",
    "format": "yarn fix:eslint && yarn fix:stylelint && yarn fix:prettier",
    "check": "yarn check:eslint && yarn check:stylelint && yarn check:type && yarn check:prettier",
    "check:eslint": "next lint -d ./ --ignore-path .gitignore --max-warnings=0",
    "check:stylelint": "stylelint --ignore-path .gitignore --max-warnings=0 \"**/*.{ts,tsx}\" && printf '\\033[32m%s\\033[m\n' 'âœ” No stylelint warnings or errors'",
    "check:type": "tsc --pretty --noEmit",
    "check:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx}\" \"firebase/**/*.{js,ts}\"",
    "fix:eslint": "yarn check:eslint --fix",
    "fix:stylelint": "stylelint --ignore-path .gitignore --max-warnings=0 \"**/*.{ts,tsx}\" --fix",
    "fix:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mdx}\" \"firebase/**/*.{js,ts}\"",
    "build:next": "next build && next export",
    "build:storybook": "storybook build -o ./build/storybook/",
    "export:firebase": "cd /home/app/firebase && firebase emulators:export --force ./db/local",
    "init:firebase": "./firebase/scripts/initFirebase.sh",
    "reset:firestore": "./firebase/scripts/resetDb.sh",
    "seeding": "ts-node -r tsconfig-paths/register -P ./firebase/tsconfig.json ./firebase/scripts/seeder.ts"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@hookform/resolvers": "3.0.1",
    "@react-google-maps/api": "^2.18.1",
    "@reduxjs/toolkit": "1.9.3",
    "aria-hidden": "^1.2.3",
    "firebase": "9.19.1",
    "firebase-admin": "^11.7.0",
    "firestore-export-import": "^1.3.6",
    "focus-trap": "^7.4.0",
    "modern-css-reset": "1.4.0",
    "next": "13.3.0",
    "next-pwa": "5.6.0",
    "next-seo": "6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "4.8.0",
    "react-redux": "8.0.5",
    "tua-body-scroll-lock": "^1.2.1",
    "typescript": "4.9.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@emotion/babel-plugin": "11.10.6",
    "@storybook/addon-actions": "7.0.2",
    "@storybook/addon-essentials": "7.0.2",
    "@storybook/addon-interactions": "7.0.2",
    "@storybook/addon-links": "7.0.2",
    "@storybook/blocks": "7.0.2",
    "@storybook/nextjs": "7.0.2",
    "@storybook/react": "7.0.2",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@types/google.maps": "^3.52.6",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "babel-loader": "9.1.2",
    "concurrently": "8.0.1",
    "eslint": "8.37.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-storybook": "0.6.11",
    "postcss": "8.4.21",
    "postcss-styled": "0.34.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.7",
    "storybook": "7.0.2",
    "stylelint": "15.4.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended": "11.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
