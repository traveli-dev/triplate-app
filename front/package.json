{
  "name": "traveli-app",
  "version": "0.1.0",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:next\" \"yarn dev:scss\" \"yarn dev:storybook\" \"yarn dev:firebase\"",
    "dev:next": "next dev",
    "dev:storybook": "start-storybook --quiet -p 6006",
    "dev:firebase": "cd /home/app/firebase && firebase emulators:start --import=./db/local",
    "dev:scss": "typed-scss-modules src --watch",
    "lint": "next lint -d ./ --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mdx,scss}\"",
    "check:lint": "next lint -d ./ --ignore-path .gitignore --max-warnings=0",
    "check:stylelint": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\"",
    "check:type": "tsc --pretty --noEmit",
    "check:format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "fix:stylelint": "stylelint --fix --ignore-path .gitignore \"**/*.{css,scss}\"",
    "build:next": "next build && next export",
    "build:scss": "typed-scss-modules src",
    "build:storybook": "build-storybook -o ./build/storybook/",
    "export:firestore": "cd /home/app/firebase && firebase emulators:export --force ./db/local",
    "clean:firestore": "rm -rf /home/app/firebase/db/local",
    "init:firebase": "./firebase/scripts/initFirebase.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.9.3",
    "firebase": "^9.16.0",
    "next": "13.0.7",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "react-icons": "^4.7.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "sass": "^1.57.1",
    "storybook-addon-next-router": "^4.0.2",
    "typescript": "4.9.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@markuplint/jsx-parser": "^3.4.0",
    "@markuplint/react-spec": "^3.4.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-storybook": "^0.6.10",
    "markuplint": "^3.4.0",
    "prettier": "^2.8.3",
    "sass-loader": "^13.2.0",
    "stylelint": "^15.3.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.5.0",
    "typed-scss-modules": "^7.0.2"
  }
}
