{
  "name": "traveli-app",
  "version": "0.1.0",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:next\" \"yarn dev:storybook\" \"yarn dev:firebase\"",
    "dev:next": "next dev",
    "dev:storybook": "storybook dev -p 6006",
    "dev:firebase": "cd /home/app/firebase && firebase emulators:start --import=./db/local",
    "format": "yarn fix:eslint && yarn fix:prettier",
    "check:lint": "next lint -d ./ --ignore-path .gitignore --max-warnings=0",
    "check:stylelint": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\"",
    "check:markuplint": "markuplint --ignore-path .gitignore \"**/*.{jsx,tsx}\"",
    "check:type": "tsc --pretty --noEmit",
    "check:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "fix:eslint": "yarn check:lint --fix",
    "fix:stylelint": "yarn check:stylelint --fix",
    "fix:markuplint": "yarn check:markuplint --fix",
    "fix:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "build:next": "next build && next export",
    "build:storybook": "storybook build -o ./build/storybook/",
    "export:firestore": "cd /home/app/firebase && firebase emulators:export --force ./db/local",
    "clean:firestore": "rm -rf /home/app/firebase/db/local",
    "init:firebase": "./firebase/scripts/initFirebase.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.9.3",
    "firebase": "^9.16.0",
    "modern-css-reset": "^1.4.0",
    "next": "13.0.7",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "typescript": "4.9.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/babel-plugin": "^11.10.6",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/nextjs": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-storybook": "^0.6.11",
    "postcss": "^8.4.21",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.3",
    "storybook": "^7.0.2"
  }
}
